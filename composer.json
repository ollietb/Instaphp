{
    "name": "instaphp/instaphp",
    "description": "A library for accessing Instagram's API (v2)",
    "keywords": ["library","instagram","instaphp"],
    "homepage": "http://instaphp.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Randy Sesser",
            "email": "randy@instaphp.com",
            "homepage": "http://instaphp.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "monolog/monolog": "~1.8",
        "psr/http-message": "^1.0",
        "php-http/client-implementation": "^1.0",
        "php-http/httplug": "^1.0",
        "php-http/discovery": "^1.0",
        "php-http/logger-plugin": "^1.0",
        "php-http/client-common": "^1.5",
        "php-http/message-factory": "^1.0",
        "php-http/message": "^1.5"
    },
    "require-dev": {
        "phpunit/phpunit": "~3.7",
        "php-http/mock-client": "^1.0",
        "php-http/curl-client": "^1.7",
        "guzzlehttp/psr7": "^1.4",
        "php-http/guzzle6-adapter": "^1.1",
        "friendsofphp/php-cs-fixer": "^2.3"
    },
    "suggest": {
        "php-http/guzzle6-adapter": "Allows you to configure the Guzzle connection options"
    }
	"autoload": {
		"psr-0": {
			"Instaphp": "src/"
		}
	}
}
